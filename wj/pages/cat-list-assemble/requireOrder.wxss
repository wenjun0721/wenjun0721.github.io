
page{
  padding-bottom: 120rpx;
}
.flex{
  display: flex;
}
.orderAddress{
  padding:20rpx 30rpx;
  position: relative;
}
.orderAddress_img{
  width:40rpx;
  height:40rpx;
  transform:rotate(180deg);
  position: absolute;
  right:30rpx;
  top:41rpx;
}
.orderAddress_userInfo{
  align-items: center;
}
.orderAddress_userInfo_text{
  color:#333;
  font-size: 30rpx;
  margin-right: 10rpx;
}
.orderAddress_userInfo_tags{
  color:#fe4848;
  font-size: 30rpx;
}

.orderAddress_userAddress{
    align-items: center;
}
.orderAddress_userAddressIcon{
  width: 24rpx;
  height: 29rpx;
}
.orderAddress_userAddressText{
  font-size: 30rpx;
  color:#666;
  padding-left: 10rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
  width:80vw;

}

.cart_list{
  padding:0 30rpx;
  border-top: 6rpx solid #eee;
  padding-bottom: 20rpx;
}
.cart_list_top{
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.cart_select_img{
  width: 40rpx;
  height: 40rpx;
}
.cart_brand_img{
  width: 40rpx;
  height: 40rpx;
}
.cart_list_top_left{
  color:#050101;
  font-size: 24rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.cart_list_top_left image{
  vertical-align: middle;
  margin-top: -2rpx;
}
.cart_list_top_right{
  color:#050101;
  font-size: 22rpx;
  height: 60rpx;
  line-height: 60rpx;
}
.cart_list_goods{
  display: flex;
  margin-bottom: 24rpx;
  position: relative;
  padding-bottom: 10rpx;
}
.noArea{
  border:1px solid red;
}
.cart_list_select{
  line-height: 180rpx;
  height: 180rpx;
}
.cart_list_select .cart_select_img{
  vertical-align: middle;
}
.cart_goods_img{
  margin-left: 10rpx;
}
.cart_goods_img image{
  width: 180rpx;
  height: 180rpx;
}
.cart_goods_title{
  color:#050101;
  font-size: 26rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
}
.cart_goods_size{
  color: #666;
  font-size: 24rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1; 
  margin-top: 10rpx;
}
.cart_goods_info{
  font-size: 26rpx;
  line-height: 1.2;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  width: 100%;
}
.cart_goods_price{
  color: #E60012;
}
.cart_goods_text{
  height: 180rpx;
  margin-left: 20rpx;
  position: relative;
  width:100%;
}

.cart_goods_num{
  align-self: flex-end;
  font-size: 24rpx;
  color:#666;
  line-height: 1.2;
  position: absolute;
  bottom: 0;
  left: 14rpx;
}
.orderOther{
  padding:0 30rpx;
}
.orderOther_list{
  justify-content: space-between;
  align-items: center;
  margin-top:20rpx;
}
.orderOther_listLeft{
  font-size: 30rpx;
  color:#050203;
}
.orderOther_listLeft_input{
  flex: 1;
  margin-left: 20rpx;
  font-size: 26rpx;
  color:#666;
}
.orderOther_listRight{
  display: flex;
  align-items: center;
}
.orderOther_listRightText{
  color:#666;
  font-size: 26rpx;
}
.cart_goods_edit{
  height: 180rpx;
  position: relative;
}
.cart_goods_edit image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top:-10rpx;
  left: 50%;
  left: 11rpx;
}
.cart_goods_num{
  align-self: flex-end;
  font-size: 24rpx;
  color:#666;
  line-height: 1.2;
  position: absolute;
  bottom: 0;
  left: 14rpx;
}
.orderOther_listRight_img{
  width: 40rpx;
  height: 40rpx;
  transform:rotate(180deg);
}
.orderOther_listRightMoney{
  color:#e30210;
  font-size: 26rpx;
}
.submitBox{
  position: fixed;
  height: 98rpx;
  line-height: 98rpx;
  bottom: 0;
  width: 100%;
  left: 0;
  border-top:1px solid #eee;
  background: #fff;
  z-index: 2;
}
.submitBox_btn{
  height: 98rpx;
  text-align: center;
  position: absolute;
  right: 0;
  bottom: 0;
  color:#fff;
  font-size: 32rpx;
  background: #e31d0b;
  width: 300rpx;
}
.submitBox_left{
  position: absolute;
  right: 340rpx;
  font-size: 32rpx;
  color:#333;
}

.picker{
  display: flex;
  align-items: center;
}